@prefix : <http://www.semanticweb.org/AstronomyOntology> .
@prefix wd: <http://www.wikidata.org/wiki/> .
@prefix dbo: <https://dbpedia.org/ontology/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix astroo: <http://www.semanticweb.org/AstronomyOntology#> .
@base <http://www.semanticweb.org/AstronomyOntology> .

<http://www.semanticweb.org/AstronomyOntology> rdf:type owl:Ontology ;
                                                owl:imports <http://www.ontologydesignpatterns.org/cp/owl/list.owl> ,
                                                            <http://www.ontologydesignpatterns.org/cp/owl/partof.owl> ;
                                                <http://purl.org/dc/elements/1.1/creator> "Edoardo Rolfo" ;
                                                <http://purl.org/dc/elements/1.1/date> "09-01-2023" ;
                                                <http://purl.org/dc/elements/1.1/description> """Ontologia ideata per modellare i principali oggetti  noti all'uomo che si possono osservare nello Spazio.
L'obietivo è quello di fornire le informazioni più rilevanti riguardo ai corpi ed agli oggetti celesti che fluttuano nello spazio, indicando le loro caratteristiche principali come il nome, la tipologia di oggetto astronomico a cui appartiene e la distanza dal Sole"""@it ;
                                                <http://purl.org/dc/elements/1.1/title> "Astronomy Ontology" ;
                                                <http://purl.org/vocab/vann/#preferredNamespacePrefix> "astroo" ,
                                                                                                       "http://www.semanticweb.org/AstronomyOntology" ;
                                                rdfs:label "AstronomyOntology" .

#################################################################
#    Annotation properties
#################################################################

###  http://purl.org/dc/elements/1.1/creator
<http://purl.org/dc/elements/1.1/creator> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/date
<http://purl.org/dc/elements/1.1/date> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/description
<http://purl.org/dc/elements/1.1/description> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/title
<http://purl.org/dc/elements/1.1/title> rdf:type owl:AnnotationProperty .


###  http://purl.org/vocab/vann/#preferredNamespacePrefix
<http://purl.org/vocab/vann/#preferredNamespacePrefix> rdf:type owl:AnnotationProperty .


###  http://purl.org/vocab/vann/#preferredNamespaceUri
<http://purl.org/vocab/vann/#preferredNamespaceUri> rdf:type owl:AnnotationProperty .


###  http://swrl.stanford.edu/ontologies/3.3/swrla.owl#isRuleEnabled
<http://swrl.stanford.edu/ontologies/3.3/swrla.owl#isRuleEnabled> rdf:type owl:AnnotationProperty .


###  http://www.ontologydesignpatterns.org/cp/owl/bag.owl#itemOf
<http://www.ontologydesignpatterns.org/cp/owl/bag.owl#itemOf> rdf:type owl:AnnotationProperty .


###  http://www.ontologydesignpatterns.org/cp/owl/sequence.owl#directlyFollows
<http://www.ontologydesignpatterns.org/cp/owl/sequence.owl#directlyFollows> rdf:type owl:AnnotationProperty .


#################################################################
#    Object Properties
#################################################################

###  http://www.ontologydesignpatterns.org/cp/owl/partof.owl#hasPart
<http://www.ontologydesignpatterns.org/cp/owl/partof.owl#hasPart> owl:equivalentProperty astroo:raggrupa ;
                                                                  rdfs:domain astroo:Galassia ;
                                                                  rdfs:range astroo:Sistema .


###  http://www.ontologydesignpatterns.org/cp/owl/partof.owl#isPartOf
<http://www.ontologydesignpatterns.org/cp/owl/partof.owl#isPartOf> rdfs:domain astroo:Sistema ;
                                                                   rdfs:range astroo:Galassia .


###  http://www.semanticweb.org/AstronomyOntology#appartieneA
astroo:appartieneA rdf:type owl:ObjectProperty ;
                   owl:inverseOf astroo:comprendePianeti ;
                   rdfs:domain astroo:Pianeta_non_interstellare ;
                   rdfs:range astroo:Sistema_planetario ;
                   owl:propertyChainAxiom ( astroo:orbitaAttornoStella
                                            astroo:centroDiGravitàDi
                                          ) ;
                   rdfs:comment "Relazione che esprime lo stato di appartenenza di un pianeta non interstellare ad un sistema planetario."@it ;
                   rdfs:label "appartieneA"@it .


###  http://www.semanticweb.org/AstronomyOntology#attrae
astroo:attrae rdf:type owl:ObjectProperty ,
                       owl:SymmetricProperty ,
                       owl:TransitiveProperty ,
                       owl:ReflexiveProperty ;
              rdfs:domain astroo:Oggetto_astronomico ;
              rdfs:range astroo:Oggetto_astronomico ;
              rdfs:comment "Relazione che esprime l'attrazione gravitazionale che un oggetto astronomico esercita su se stesso e sugli altri."@it ;
              rdfs:label "attrae"@it .


###  http://www.semanticweb.org/AstronomyOntology#centroDiGravitàDi
astroo:centroDiGravitàDi rdf:type owl:ObjectProperty ;
                         owl:inverseOf astroo:formatoAttornoA ;
                         rdfs:domain astroo:Stella ;
                         rdfs:range astroo:Sistema_planetario ;
                         rdfs:comment "Relazione che definisce una stella come centro di gravità attorno a cui si è formato un sistema planetario."@it ,
                                      "centroDiGravitàDi"@it .


###  http://www.semanticweb.org/AstronomyOntology#comprendePianeti
astroo:comprendePianeti rdf:type owl:ObjectProperty ;
                        rdfs:domain astroo:Sistema_planetario ;
                        rdfs:range astroo:Pianeta_non_interstellare ;
                        rdfs:comment "Relazione che definisce quali pianeti un sistema plaentario comprende."@it ;
                        rdfs:label "comprendePianeti"@it .


###  http://www.semanticweb.org/AstronomyOntology#faOrbitarePianeta
astroo:faOrbitarePianeta rdf:type owl:ObjectProperty ;
                         owl:inverseOf astroo:orbitaAttornoStella ;
                         rdfs:domain astroo:Stella ;
                         rdfs:range astroo:Pianeta_non_interstellare ;
                         rdfs:comment "Relazione che definisce i pianeti che orbitano attorno ad una stella."@it ;
                         rdfs:label "faOrbitarePianeta"@it .


###  http://www.semanticweb.org/AstronomyOntology#faOrbitareSatellite
astroo:faOrbitareSatellite rdf:type owl:ObjectProperty ;
                           owl:inverseOf astroo:orbitaAttornoPianeta ;
                           rdfs:domain astroo:Pianeta ;
                           rdfs:range astroo:Satellite_naturale ;
                           rdfs:comment "Relazione che definisce come satellite un corpo celeste orbitante attorno ad un pianeta."@it ;
                           rdfs:label "faOrbitareSatellite"@it .


###  http://www.semanticweb.org/AstronomyOntology#fluttuaIn
astroo:fluttuaIn rdf:type owl:ObjectProperty ;
                 rdfs:domain astroo:Pianeta_non_interstellare ;
                 rdfs:range astroo:Galassia ;
                 owl:propertyChainAxiom ( astroo:orbitaAttornoStella
                                          astroo:centroDiGravitàDi
                                          astroo:situatoIn
                                        ) ;
                 rdfs:comment "Relazione che esprime in quale galassia fluttua un pianeta."@it ;
                 rdfs:label "fluttuaIn"@it .


###  http://www.semanticweb.org/AstronomyOntology#formaSistemaStellare
astroo:formaSistemaStellare rdf:type owl:ObjectProperty ;
                            owl:inverseOf astroo:insiemeDiStelle ;
                            rdfs:domain astroo:Stella ;
                            rdfs:range astroo:Sistema_stellare ;
                            rdfs:comment "Relazione che definisce una stella come parte dell'insieme di stelle di un sistema stellare."@it ;
                            rdfs:label "formaSistemaStellare"@it .


###  http://www.semanticweb.org/AstronomyOntology#formatoAttornoA
astroo:formatoAttornoA rdf:type owl:ObjectProperty ;
                       rdfs:domain astroo:Sistema_planetario ;
                       rdfs:range astroo:Stella ;
                       rdfs:comment "Relazione che definisce attorno a quale stella si è formato il sistema planetario."@it ;
                       rdfs:label "formatoAttornoA"@it .


###  http://www.semanticweb.org/AstronomyOntology#formatoDaAltriCorpi
astroo:formatoDaAltriCorpi rdf:type owl:ObjectProperty ;
                           owl:inverseOf astroo:orbitaInSistemaPlanetario ;
                           rdfs:domain astroo:Sistema_planetario ;
                           rdfs:range [ owl:intersectionOf ( astroo:Corpo_celeste
                                                             [ rdf:type owl:Class ;
                                                               owl:complementOf astroo:Pianeta_interstellare
                                                             ]
                                                             [ rdf:type owl:Class ;
                                                               owl:complementOf astroo:Stella
                                                             ]
                                                           ) ;
                                        rdf:type owl:Class
                                      ] ;
                           rdfs:comment "Relazione che definisce gli altri corpi celesti, ad eccezione delle stelle e pianeti, che formano un sistema planetario."@it ;
                           rdfs:label "formatoDaAltriCorpi"@it .


###  http://www.semanticweb.org/AstronomyOntology#illuminaPianeta
astroo:illuminaPianeta rdf:type owl:ObjectProperty ;
                       rdfs:comment "Relazione che esprime quando una stella illumina direttamente un pianeta."@it ;
                       rdfs:label "illuminaPianeta"@it .


###  http://www.semanticweb.org/AstronomyOntology#insiemeDiStelle
astroo:insiemeDiStelle rdf:type owl:ObjectProperty ;
                       rdfs:domain astroo:Sistema_stellare ;
                       rdfs:range astroo:Stella ;
                       rdfs:comment "Relazione che definisce le stelle facenti parte di un sistema stellare."@it ;
                       rdfs:label "insiemeDiStelle"@it .


###  http://www.semanticweb.org/AstronomyOntology#orbitaAttornoPianeta
astroo:orbitaAttornoPianeta rdf:type owl:ObjectProperty ;
                            rdfs:domain astroo:Satellite_naturale ;
                            rdfs:range astroo:Pianeta ;
                            rdfs:comment "Relazione che definisce il pianeta attorno a cui un satellite orbita."@it ;
                            rdfs:label "orbitaAttornoPianeta"@it .


###  http://www.semanticweb.org/AstronomyOntology#orbitaAttornoStella
astroo:orbitaAttornoStella rdf:type owl:ObjectProperty ;
                           rdfs:domain astroo:Pianeta_non_interstellare ;
                           rdfs:range astroo:Stella ;
                           rdfs:comment "Relazione che definisce attorno a quale stella orbita un pianeta."@it ;
                           rdfs:label "orbitaAttornoStella"@it .


###  http://www.semanticweb.org/AstronomyOntology#orbitaInSistemaPlanetario
astroo:orbitaInSistemaPlanetario rdf:type owl:ObjectProperty ;
                                 rdfs:domain [ owl:intersectionOf ( astroo:Corpo_celeste
                                                                    [ rdf:type owl:Class ;
                                                                      owl:complementOf astroo:Pianeta_interstellare
                                                                    ]
                                                                    [ rdf:type owl:Class ;
                                                                      owl:complementOf astroo:Stella
                                                                    ]
                                                                  ) ;
                                               rdf:type owl:Class
                                             ] ;
                                 rdfs:range astroo:Sistema_planetario ;
                                 rdfs:comment "Relazione che definisce in quale sistema planetario i corpi celesti, ad eccezione del centro di gravità, orbinato."@it ;
                                 rdfs:label "orbitaInSistemaPlanetario"@it .


###  http://www.semanticweb.org/AstronomyOntology#orbitaNeiDintorniDi
astroo:orbitaNeiDintorniDi rdf:type owl:ObjectProperty ,
                                    owl:TransitiveProperty ;
                           rdfs:domain astroo:Corpo_celeste_non_sferoidale ;
                           rdfs:range astroo:Corpo_celeste ;
                           rdfs:comment "Relazione che esprime nei dintorni di quale corpo celeste i corpi non sferoidali orbitano."@it ;
                           rdfs:label "orbitaNeiDintorniDi"@it .


###  http://www.semanticweb.org/AstronomyOntology#raggrupa
astroo:raggrupa rdf:type owl:ObjectProperty ;
                owl:inverseOf astroo:situatoIn ;
                rdfs:domain astroo:Galassia ;
                rdfs:range astroo:Sistema ;
                rdfs:comment "Relazione che definisce la galassia come raggruppamento di sistemi."@it ;
                rdfs:label "raggruppa"@it .


###  http://www.semanticweb.org/AstronomyOntology#satelliteInSistema
astroo:satelliteInSistema rdf:type owl:ObjectProperty ;
                          rdfs:comment "Relazione che esprime la presenza del satellite in un sistema."@it ;
                          rdfs:label "satelliteInSistema"@it .


###  http://www.semanticweb.org/AstronomyOntology#situatoIn
astroo:situatoIn rdf:type owl:ObjectProperty ,
                          owl:FunctionalProperty ;
                 rdfs:domain astroo:Sistema ;
                 rdfs:range astroo:Galassia ;
                 rdfs:comment "Relazione che definisce in quale galassia un sistema è situato."@it ;
                 rdfs:label "situatoIn"@it .


#################################################################
#    Data properties
#################################################################

###  http://www.semanticweb.org/AstronomyOntology#composizionePianeta
astroo:composizionePianeta rdf:type owl:DatatypeProperty ;
                           rdfs:domain astroo:Pianeta ;
                           rdfs:range [ rdf:type rdfs:Datatype ;
                                        owl:oneOf [ rdf:type rdf:List ;
                                                    rdf:first "Gassosa" ;
                                                    rdf:rest [ rdf:type rdf:List ;
                                                               rdf:first "Rocciosa" ;
                                                               rdf:rest rdf:nil
                                                             ]
                                                  ]
                                      ] ;
                           rdfs:comment "Proprietà che esprime quale tipo di materia compone la maggior parte della massa di un pianeta."@it ;
                           rdfs:label "composizione pianeta"@it .


###  http://www.semanticweb.org/AstronomyOntology#distanzaDalSole
astroo:distanzaDalSole rdf:type owl:DatatypeProperty ;
                       rdfs:domain astroo:Oggetto_astronomico ;
                       rdfs:range xsd:float ;
                       rdfs:comment "Proprietà che descrive la distanza di un oggeto dal Sole in \"anni luce\"."@it ;
                       rdfs:label "distanza dal sole"@it .


###  http://www.semanticweb.org/AstronomyOntology#emetteLuce
astroo:emetteLuce rdf:type owl:DatatypeProperty ;
                  rdfs:domain astroo:Corpo_celeste ;
                  rdfs:range xsd:boolean ;
                  rdfs:comment "Proprietà che esprime se un corpo celeste emtte luce propria o no."@it ;
                  rdfs:label "emette luce"@it .


###  http://www.semanticweb.org/AstronomyOntology#formaSferoidale
astroo:formaSferoidale rdf:type owl:DatatypeProperty ;
                       rdfs:domain astroo:Corpo_celeste ;
                       rdfs:range xsd:boolean ;
                       rdfs:comment "Proprietà che descrive la forma di un corpo celeste."@it ;
                       rdfs:label "forma sferoidale"@it .


###  http://www.semanticweb.org/AstronomyOntology#haOrbita
astroo:haOrbita rdf:type owl:DatatypeProperty ;
                rdfs:domain astroo:Pianeta ;
                rdfs:range [ rdf:type rdfs:Datatype ;
                             owl:oneOf [ rdf:type rdf:List ;
                                         rdf:first "Inesistente" ;
                                         rdf:rest [ rdf:type rdf:List ;
                                                    rdf:first "Libera" ;
                                                    rdf:rest [ rdf:type rdf:List ;
                                                               rdf:first "Non libera" ;
                                                               rdf:rest rdf:nil
                                                             ]
                                                  ]
                                       ]
                           ] ;
                rdfs:comment "Proprietà che esprime lo stato dell'orbita di un pianeta."@it ;
                rdfs:label "ha orbita"@it .


###  http://www.semanticweb.org/AstronomyOntology#nome
astroo:nome rdf:type owl:DatatypeProperty ;
            rdfs:domain astroo:Oggetto_astronomico ;
            rdfs:range xsd:string ;
            rdfs:comment "Proprietà che esprime il nome di un oggetto astronomico."@it ;
            rdfs:label "nome"@it .


###  http://www.semanticweb.org/AstronomyOntology#stadioVitale
astroo:stadioVitale rdf:type owl:DatatypeProperty ;
                    rdfs:domain astroo:Stella ;
                    rdfs:range [ rdf:type rdfs:Datatype ;
                                 owl:oneOf [ rdf:type rdf:List ;
                                             rdf:first "Degenere" ;
                                             rdf:rest [ rdf:type rdf:List ;
                                                        rdf:first "Stabile" ;
                                                        rdf:rest rdf:nil
                                                      ]
                                           ]
                               ] ;
                    rdfs:comment "Proprietà che esprime in quale stadio vitale si trova una stella."@it ;
                    rdfs:label "stadio vitale"@it .


###  http://www.semanticweb.org/AstronomyOntology#temperatura
astroo:temperatura rdf:type owl:DatatypeProperty ;
                   rdfs:domain astroo:Stella ;
                   rdfs:range xsd:double ;
                   rdfs:comment "Proprietà che esprime la temperatura superficiale di una stella in Kelvin."@it ;
                   rdfs:label "temperatura superficiale"@it .


#################################################################
#    Classes
#################################################################

###  http://www.semanticweb.org/AstronomyOntology#Classe_A
astroo:Classe_A rdf:type owl:Class ;
                owl:equivalentClass [ owl:intersectionOf ( astroo:Stella
                                                           [ rdf:type owl:Restriction ;
                                                             owl:onProperty astroo:temperatura ;
                                                             owl:someValuesFrom [ rdf:type rdfs:Datatype ;
                                                                                  owl:onDatatype xsd:double ;
                                                                                  owl:withRestrictions ( [ xsd:minInclusive "7500.0"^^xsd:double
                                                                                                         ]
                                                                                                         [ xsd:maxExclusive "10000.0"^^xsd:double
                                                                                                         ]
                                                                                                       )
                                                                                ]
                                                           ]
                                                         ) ;
                                      rdf:type owl:Class
                                    ] ;
                rdfs:comment "Classe che raggruppa le stelle mature classificate nella classe A secondo la classificazione di Harvard"@it ;
                rdfs:label "Stella bianca (A)"@it .


###  http://www.semanticweb.org/AstronomyOntology#Classe_B
astroo:Classe_B rdf:type owl:Class ;
                owl:equivalentClass [ owl:intersectionOf ( astroo:Stella
                                                           [ rdf:type owl:Restriction ;
                                                             owl:onProperty astroo:temperatura ;
                                                             owl:someValuesFrom [ rdf:type rdfs:Datatype ;
                                                                                  owl:onDatatype xsd:double ;
                                                                                  owl:withRestrictions ( [ xsd:minInclusive "10000.0"^^xsd:double
                                                                                                         ]
                                                                                                         [ xsd:maxExclusive "33000.0"^^xsd:double
                                                                                                         ]
                                                                                                       )
                                                                                ]
                                                           ]
                                                         ) ;
                                      rdf:type owl:Class
                                    ] ;
                rdfs:comment "Classe che raggruppa le stelle mature classificate nella classe B secondo la classificazione di Harvard."@it ;
                rdfs:label "Stella azzurra (B)"@it .


###  http://www.semanticweb.org/AstronomyOntology#Classe_F
astroo:Classe_F rdf:type owl:Class ;
                owl:equivalentClass [ owl:intersectionOf ( astroo:Stella
                                                           [ rdf:type owl:Restriction ;
                                                             owl:onProperty astroo:temperatura ;
                                                             owl:someValuesFrom [ rdf:type rdfs:Datatype ;
                                                                                  owl:onDatatype xsd:double ;
                                                                                  owl:withRestrictions ( [ xsd:minInclusive "6000.0"^^xsd:double
                                                                                                         ]
                                                                                                         [ xsd:maxExclusive "7500.0"^^xsd:double
                                                                                                         ]
                                                                                                       )
                                                                                ]
                                                           ]
                                                         ) ;
                                      rdf:type owl:Class
                                    ] ;
                rdfs:comment "Classe che raggruppa le stelle mature classificate nella classe F secondo la classificazione di Harvard"@it ;
                rdfs:label "Stella bianco-gialla (F)"@it .


###  http://www.semanticweb.org/AstronomyOntology#Classe_G
astroo:Classe_G rdf:type owl:Class ;
                owl:equivalentClass [ owl:intersectionOf ( astroo:Stella
                                                           [ rdf:type owl:Restriction ;
                                                             owl:onProperty astroo:temperatura ;
                                                             owl:someValuesFrom [ rdf:type rdfs:Datatype ;
                                                                                  owl:onDatatype xsd:double ;
                                                                                  owl:withRestrictions ( [ xsd:minInclusive "5200.0"^^xsd:double
                                                                                                         ]
                                                                                                         [ xsd:maxExclusive "6000.0"^^xsd:double
                                                                                                         ]
                                                                                                       )
                                                                                ]
                                                           ]
                                                         ) ;
                                      rdf:type owl:Class
                                    ] ;
                rdfs:comment "Classe che raggruppa le stelle mature classificate nella classe G secondo la classificazione di Harvard."@it ;
                rdfs:label "Stella gialla (G)"@it .


###  http://www.semanticweb.org/AstronomyOntology#Classe_K
astroo:Classe_K rdf:type owl:Class ;
                owl:equivalentClass [ owl:intersectionOf ( astroo:Stella
                                                           [ rdf:type owl:Restriction ;
                                                             owl:onProperty astroo:temperatura ;
                                                             owl:someValuesFrom [ rdf:type rdfs:Datatype ;
                                                                                  owl:onDatatype xsd:double ;
                                                                                  owl:withRestrictions ( [ xsd:minInclusive "3700.0"^^xsd:double
                                                                                                         ]
                                                                                                         [ xsd:maxExclusive "5200.0"^^xsd:double
                                                                                                         ]
                                                                                                       )
                                                                                ]
                                                           ]
                                                         ) ;
                                      rdf:type owl:Class
                                    ] ;
                rdfs:subClassOf astroo:Stella ;
                rdfs:comment "Classe che raggruppa le stelle mature classificate nella classe K secondo la classificazione di Harvard."@it ;
                rdfs:label "Stella arancione (K)"@it .


###  http://www.semanticweb.org/AstronomyOntology#Classe_M
astroo:Classe_M rdf:type owl:Class ;
                owl:equivalentClass [ owl:intersectionOf ( astroo:Stella
                                                           [ rdf:type owl:Restriction ;
                                                             owl:onProperty astroo:temperatura ;
                                                             owl:someValuesFrom [ rdf:type rdfs:Datatype ;
                                                                                  owl:onDatatype xsd:double ;
                                                                                  owl:withRestrictions ( [ xsd:maxExclusive "3700.0"^^xsd:double
                                                                                                         ]
                                                                                                       )
                                                                                ]
                                                           ]
                                                         ) ;
                                      rdf:type owl:Class
                                    ] ;
                rdfs:subClassOf astroo:Stella ;
                rdfs:comment "Classe che raggruppa le stelle mature classificate nella classe M secondo la classificazione di Harvard."@it ;
                rdfs:label "Stella rossa (M)"@it .


###  http://www.semanticweb.org/AstronomyOntology#Classe_O
astroo:Classe_O rdf:type owl:Class ;
                owl:equivalentClass [ owl:intersectionOf ( astroo:Stella
                                                           [ rdf:type owl:Restriction ;
                                                             owl:onProperty astroo:temperatura ;
                                                             owl:someValuesFrom [ rdf:type rdfs:Datatype ;
                                                                                  owl:onDatatype xsd:double ;
                                                                                  owl:withRestrictions ( [ xsd:minInclusive "33000.0"^^xsd:double
                                                                                                         ]
                                                                                                       )
                                                                                ]
                                                           ]
                                                         ) ;
                                      rdf:type owl:Class
                                    ] ;
                rdfs:subClassOf astroo:Stella ;
                rdfs:comment "Classe che raggruppa le stelle mature classificate nella classe O secondo la classificazione di Harvard."@it ;
                rdfs:label "Stella blu (O)"@it .


###  http://www.semanticweb.org/AstronomyOntology#Corpo_celeste
astroo:Corpo_celeste rdf:type owl:Class ;
                     rdfs:subClassOf astroo:Oggetto_astronomico ;
                     owl:disjointWith astroo:Oggetto_celeste ,
                                      astroo:Sistema ;
                     rdfs:comment "Classe che identifica i corpi celesti intesi come strutture unitarie e coesive tenute assieme dalla forza di gravità"@it ;
                     rdfs:label "Corpo celeste"@it .


###  http://www.semanticweb.org/AstronomyOntology#Corpo_celeste_non_sferoidale
astroo:Corpo_celeste_non_sferoidale rdf:type owl:Class ;
                                    rdfs:subClassOf astroo:Corpo_celeste ;
                                    rdfs:comment "Classe che raggruppa i corpi celesti non sferoidali."@it ;
                                    rdfs:label "Corpo celeste non sferoidale"@it .


