                                                                                                              owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                                                                              owl:onClass astroo:Pianeta
                                                                                                            ]
                                                                                                          ) ;
                                                                                       rdf:type owl:Class
                                                                                     ]
                                                                                     [ rdf:type owl:Restriction ;
                                                                                       owl:onProperty astroo:emetteLuce ;
